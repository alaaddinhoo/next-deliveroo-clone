{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      // "@imgs/*": ["./images/*"],
      "@logo/*": ["./images/logos/*"],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/restaurants/components/TopPicks.tsx",
    "middleware.ts",
    "hooks/useEmailVerification.js",
    "components/ProtectedRoute.tsx",
    "components/ProtectedRouter.jsx",
    "utils/http.ts"
  ],"exclude": ["node_modules"]
}
